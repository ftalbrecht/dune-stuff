dimDomain = 1, 2, 3 | expand

geometry = cube, simplex | expand 2
geometry_is_simplex = false, true | expand 2
refinement = nonconforming, conforming | expand

grid_yasp = Dune::YaspGrid<{dimDomain},Dune::EquidistantOffsetCoordinates<double,{dimDomain}>>
grid_alu = Dune::ALUGrid<{dimDomain},{dimDomain},Dune::{geometry},Dune::{refinement}>
grid = {grid_yasp}, {grid_alu}  | expand grid
'{grid}' == 'Dune::ALUGrid<1,1,Dune::{geometry},Dune::{refinement}>' | exclude
'{grid}' == 'Dune::ALUGrid<{dimDomain},{dimDomain},Dune::cube,Dune::conforming>' | exclude
'{grid}' == 'Dune::YaspGrid<{dimDomain},Dune::EquidistantOffsetCoordinates<double,{dimDomain}>>' and ('{refinement}' == 'conforming' or '{geometry}' == 'simplex') | exclude

entity_type = {grid}::Codim<0>::Entity

1, ALUGRID_FOUND | expand grid | cmake_guard


